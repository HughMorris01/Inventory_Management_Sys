      <Label layoutX="75.0" layoutY="103.0" text="ID:" />
      <Label layoutX="75.0" layoutY="143.0" prefHeight="18.0" prefWidth="34.0" text="Name:" />
      <Label layoutX="294.0" layoutY="264.0" text="Maximum:" />
