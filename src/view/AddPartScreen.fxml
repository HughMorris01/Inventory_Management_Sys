      <Label layoutX="75.0" layoutY="223.0" text="Price:" />
      <Label layoutX="75.0" layoutY="263.0" text="Minimum:" />
      <Label fx:id="radioButtonLabel" layoutX="75.0" layoutY="303.0" text="Machine ID:" />
      <TextField fx:id="iDTextField" disable="true" editable="false" layoutX="180.0" layoutY="100.0" prefWidth="125.0" promptText="Auto Gen - Disabled" />
      <TextField fx:id="nameTextField" layoutX="180.0" layoutY="140.0" prefWidth="125.0" />
      <TextField fx:id="maxTextField" layoutX="379.0" layoutY="260.0" prefHeight="26.0" prefWidth="85.0" />
      <TextField fx:id="inventoryTextField" layoutX="180.0" layoutY="180.0" prefWidth="125.0" />
      <TextField fx:id="priceTextField" layoutX="180.0" layoutY="220.0" prefWidth="125.0" />
      <TextField fx:id="minTextField" layoutX="180.0" layoutY="260.0" prefHeight="26.0" prefWidth="85.0" />
      <TextField fx:id="radioButtonTextField" layoutX="180.0" layoutY="300.0" prefWidth="125.0" />
   </children>
</AnchorPane>
